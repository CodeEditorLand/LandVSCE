{
	"name": "@vscode/vsce",
	"version": "0.0.0",
	"description": "VS Code Extensions Manager",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vsce"
	},
	"homepage": "https://code.visualstudio.com",
	"bugs": "https://github.com/Microsoft/vsce/issues",
	"keywords": [
		"vscode",
		"vsce",
		"extension"
	],
	"publishConfig": {
		"access": "public"
	},
	"contributors": [
		"Microsoft Corporation"
	],
	"author": "Microsoft Corporation",
	"license": "MIT",
	"main": "out/api.js",
	"typings": "dist/vsce.d.ts",
	"bin": {
		"vsce": "vsce"
	},
	"scripts": {
		"compile": "tsc",
		"api": "api-extractor run --local --verbose",
		"build": "npm run compile && npm run api",
		"watch:build": "npm run compile -- --watch",
		"test": "mocha",
		"watch:test": "npm run test -- --watch"
	},
	"engines": {
		"node": ">= 14"
	},
	"dependencies": {
		"azure-devops-node-api": "12.5.0",
		"chalk": "5.3.0",
		"cheerio": "1.0.0-rc.12",
		"commander": "12.0.0",
		"glob": "10.3.10",
		"hosted-git-info": "7.0.1",
		"jsonc-parser": "3.2.1",
		"leven": "4.0.0",
		"markdown-it": "14.0.0",
		"mime": "4.0.1",
		"minimatch": "9.0.3",
		"parse-semver": "1.1.1",
		"read": "3.0.1",
		"semver": "7.6.0",
		"tmp": "0.2.3",
		"typed-rest-client": "1.8.11",
		"url-join": "5.0.0",
		"xml2js": "0.6.2",
		"yauzl": "3.1.2",
		"yazl": "2.5.1"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "7.42.3",
		"@types/cheerio": "0.22.35",
		"@types/glob": "8.1.0",
		"@types/hosted-git-info": "3.0.5",
		"@types/markdown-it": "13.0.7",
		"@types/mime": "3",
		"@types/minimatch": "5.1.2",
		"@types/mocha": "10.0.6",
		"@types/node": "20.11.28",
		"@types/read": "0.0.32",
		"@types/semver": "7.5.8",
		"@types/tmp": "0.2.6",
		"@types/url-join": "4.0.3",
		"@types/xml2js": "0.4.14",
		"@types/yauzl": "2.10.3",
		"@types/yazl": "2.4.5",
		"mocha": "10.3.0",
		"source-map-support": "0.5.21",
		"ts-node": "10.9.2",
		"typescript": "5.4.2"
	},
	"optionalDependencies": {
		"keytar": "7.9.0"
	},
	"mocha": {
		"require": [
			"ts-node/register"
		],
		"watch-files": "src/**",
		"spec": "src/test/**/*.ts"
	}
}
