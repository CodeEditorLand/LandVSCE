{
  "name": "node-debug",
  "displayName": "Node Debug",
  "version": "1.6.2",
  "private": true,
  "description": "%extension.description%",
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-node-debug/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-node-debug.git"
  },
  "license": "MIT",
  "author": {
    "name": "Microsoft Corporation"
  },
  "publisher": "ms-vscode",
  "main": "./out/node/extension",
  "scripts": {
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "nodemon": "./node_modules/.bin/nodemon --debug --nolazy ./out/node/nodeDebug.js --server=4711",
    "prepublish": "node ./node_modules/gulp/bin/gulp.js build"
  },
  "contributes": {
    "breakpoints": [
      {
        "language": "javascript"
      },
      {
        "language": "javascriptreact"
      }
    ],
    "debuggers": [
      {
        "configurationAttributes": {
          "attach": {
            "properties": {
              "address": {
                "default": "localhost",
                "description": "%node.address.description%",
                "type": "string"
              },
              "localRoot": {
                "default": null,
                "description": "%node.attach.localRoot.description%",
                "type": [
                  "string",
                  "null"
                ]
              },
              "outDir": {
                "default": null,
                "description": "%node.outDir.description%",
                "type": [
                  "string",
                  "null"
                ]
              },
              "outFiles": {
                "default": [],
                "description": "%node.outFiles.description%",
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "port": {
                "default": 5858,
                "description": "%node.port.description%",
                "type": "number"
              },
              "processId": {
                "default": "${command.PickProcess}",
                "description": "%node.attach.processId.description%",
                "type": "string"
              },
              "remoteRoot": {
                "default": null,
                "description": "%node.attach.remoteRoot.description%",
                "type": [
                  "string",
                  "null"
                ]
              },
              "restart": {
                "default": true,
                "description": "%node.attach.restart.description%",
                "type": "boolean"
              },
              "smartStep": {
                "default": true,
                "description": "%node.smartStep.description%",
                "type": "boolean"
              },
              "sourceMaps": {
                "default": true,
                "description": "%node.sourceMaps.description%",
                "type": "boolean"
              },
              "stopOnEntry": {
                "default": true,
                "description": "%node.stopOnEntry.description%",
                "type": "boolean"
              },
              "timeout": {
                "default": 10000,
                "description": "%node.timeout.description%",
                "type": "number"
              }
            }
          },
          "launch": {
            "properties": {
              "address": {
                "default": "localhost",
                "description": "%node.address.description%",
                "type": "string"
              },
              "args": {
                "default": [],
                "description": "%node.launch.args.description%",
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "console": {
                "default": "internalConsole",
                "description": "%node.launch.console.description%",
                "enum": [
                  "internalConsole",
                  "integratedTerminal",
                  "externalTerminal"
                ]
              },
              "cwd": {
                "default": "${workspaceRoot}",
                "description": "%node.launch.cwd.description%",
                "type": "string"
              },
              "env": {
                "additionalProperties": {
                  "type": "string"
                },
                "default": {},
                "description": "%node.launch.env.description%",
                "type": "object"
              },
              "externalConsole": {
                "default": true,
                "description": "%node.launch.externalConsole.description%",
                "enum": [],
                "errorMessage": "%node.launch.externalConsole.error%",
                "type": "boolean"
              },
              "outDir": {
                "default": null,
                "description": "%node.outDir.description%",
                "type": [
                  "string",
                  "null"
                ]
              },
              "outFiles": {
                "default": [],
                "description": "%node.outFiles.description%",
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "port": {
                "default": 5858,
                "description": "%node.port.description%",
                "type": "number"
              },
              "program": {
                "description": "%node.launch.program.description%",
                "type": "string"
              },
              "runtimeArgs": {
                "default": [],
                "description": "%node.launch.runtimeArgs.description%",
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "runtimeExecutable": {
                "default": null,
                "description": "%node.launch.runtimeExecutable.description%",
                "type": [
                  "string",
                  "null"
                ]
              },
              "smartStep": {
                "default": true,
                "description": "%node.smartStep.description%",
                "type": "boolean"
              },
              "sourceMaps": {
                "default": true,
                "description": "%node.sourceMaps.description%",
                "type": "boolean"
              },
              "stopOnEntry": {
                "default": true,
                "description": "%node.stopOnEntry.description%",
                "type": "boolean"
              },
              "timeout": {
                "default": 10000,
                "description": "%node.timeout.description%",
                "type": "number"
              }
            },
            "required": [
              "cwd"
            ]
          }
        },
        "initialConfigurations": "extension.provideInitialConfigurations",
        "label": "%node.label%",
        "program": "./out/node/nodeDebug.js",
        "runtime": "node",
        "type": "node",
        "variables": {
          "PickProcess": "extension.pickNodeProcess"
        }
      },
      {
        "configurationAttributes": {
          "launch": {
            "properties": {
              "args": {
                "default": [
                  "--extensionDevelopmentPath=${workspaceRoot}"
                ],
                "description": "%extensionHost.launch.args.description%",
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "outDir": {
                "default": "out",
                "description": "%extensionHost.launch.outDir.description%",
                "type": [
                  "string",
                  "null"
                ]
              },
              "outFiles": {
                "default": [],
                "description": "%extensionHost.launch.outFiles.description%",
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "runtimeExecutable": {
                "default": "${execPath}",
                "description": "%extensionHost.launch.runtimeExecutable.description%",
                "type": [
                  "string",
                  "null"
                ]
              },
              "smartStep": {
                "default": true,
                "description": "%extensionHost.smartStep.description%",
                "type": "boolean"
              },
              "sourceMaps": {
                "default": true,
                "description": "%extensionHost.launch.sourceMaps.description%",
                "type": "boolean"
              },
              "stopOnEntry": {
                "default": true,
                "description": "%extensionHost.launch.stopOnEntry.description%",
                "type": "boolean"
              }
            },
            "required": [
              "runtimeExecutable",
              "args"
            ]
          }
        },
        "initialConfigurations": [
          {
            "args": [
              "--extensionDevelopmentPath=${workspaceRoot}"
            ],
            "name": "%extensionHost.launch.config.name%",
            "outDirs": [
              "${workspaceRoot}/out/**/*.js"
            ],
            "preLaunchTask": "npm",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "sourceMaps": true,
            "stopOnEntry": false,
            "type": "extensionHost"
          }
        ],
        "label": "%extensionHost.label%",
        "program": "./out/node/nodeDebug.js",
        "runtime": "node",
        "type": "extensionHost"
      }
    ]
  },
  "activationEvents": [
    "onCommand:extension.pickNodeProcess",
    "onCommand:extension.provideInitialConfigurations"
  ],
  "dependencies": {
    "glob": "7.0.6",
    "request-light": "0.1.0",
    "source-map": "0.5.3",
    "vscode-debugadapter": "1.13.0-pre.4",
    "vscode-debugprotocol": "1.13.0-pre.5",
    "vscode-nls": "2.0.0"
  },
  "devDependencies": {
    "@types/es6-collections": "0.5.29",
    "@types/es6-promise": "0.0.32",
    "@types/mocha": "2.2.32",
    "@types/node": "6.0.39",
    "@types/source-map": "0.1.27",
    "del": "*",
    "git-rev-sync": "*",
    "gulp": "3.9.1",
    "gulp-azure-storage": "*",
    "gulp-filter": "3.0.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-tsb": "2.0.0",
    "gulp-tslint": "6.1.1",
    "gulp-typescript": "2.14.0",
    "gulp-uglify": "2.0.0",
    "gulp-util": "3.0.7",
    "gulp-vinyl-zip": "*",
    "mocha": "2.4.5",
    "nodemon": "1.10.2",
    "run-sequence": "*",
    "tslint-microsoft-contrib": "2.0.10",
    "typescript": "2.0.2",
    "vsce": "1.12.0",
    "vscode": "0.11.18",
    "vscode-debugadapter-testsupport": "1.13.0-pre.3",
    "vscode-nls-dev": "2.0.0-beta.1"
  }
}
